copy platform_alerting_service_db.pas_condition_metadata TO 'ConditionMetaData_DUMP.csv' WITH HEADER=TRUE ;
DROP TABLE platform_alerting_service_db.pas_condition_metadata;
CREATE TABLE IF NOT EXISTS platform_alerting_service_db.pas_condition_metadata (
    conditionid text Primary key,
	conditionname text,
	conditionfamily text,
	closingcondition boolean,
	underresearch boolean,
	downstream text,
	fetchdata boolean,
	remediation boolean,
	consolidation boolean,
	consolidationsleeptime text,
	issnooze boolean,
	snooze text,
	conditionpayload text,
	sourceSystem text,
	updatedtime timestamp,
    priority int,
    description text,
    configurable boolean
);
copy platform_alerting_service_db.pas_condition_metadata FROM 'ConditionMetaData_DUMP.csv' WITH HEADER=TRUE ;
